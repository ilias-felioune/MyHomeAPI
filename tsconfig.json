{
    "compilerOptions": {
      "strict": true,
      "module": "commonjs",
      "noImplicitAny": true,
      "preserveConstEnums": true,
      "esModuleInterop": true,
      "target": "es2017",
      "moduleResolution": "node",
      "outDir": "./dist",
      "baseUrl": "./",
      "paths": {
        "@/*": [
          "./src/*"
        ]
      },
    },
    "exclude": [
      "node_modules",
      "src/private/**/*"
    ]
}